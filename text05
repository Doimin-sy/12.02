#include <stdio.h>
int main(int argc, char const *argv[])
{
    // for(초기값;조건;증가값){반복할 로직} 
    for(int i=1;i<=10;i+=2)
    {
        printf("%d\n",i);
    } 
    for(int i=0;i<10;i+=1)
    {
        for(int j=0;j<5;j++){
            printf("i:%d * j:%d = %d\n",i,j,i*j);
        }
    }
    for(int i=1;i<=3;i+=1)
    {
        for(int j=1;j<=5;j++){
            printf("*");
        }
        printf("\n");
    }
    for(int i=0;i<2;i++){
        for(int j=0;j<3;j++){
            printf("!");
        }
        printf("\n");
    }
    for(int i=0;i<5;i+=1)
    {
        for(int j=0;j<5;j++){
            printf("*");
        }
        printf("\n");
    }
    for(int i=0;i<5;i+=1){
        for(int j=0;j<5-i;j++)
        {
            printf("!");
        }
        printf("\n");
    }
    for(int i=0;i<5;i+=1){
        for(int j=0;j<1+i;j++)
        {
            printf("*");
        }
        printf("\n");
    }
    for(int i=1;i<=9;i+=1)
    {
        for(int j=2;j<=9;j++)
        {
            printf("%3d x %3d = %3d ",j,i,i*j);
        }
        printf("\n");
    }
    for(int i=1;i<=9;i+=1)
    {
        for(int j=3;j<=7;j++)
        {
            printf("%3d x %3d = %3d ",j,i,i*j);
        }
        printf("\n");
    }

    return 0;
}
